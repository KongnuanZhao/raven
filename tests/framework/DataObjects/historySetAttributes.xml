<?xml version="1.0" ?>
<Simulation verbosity="debug">
  <TestInfo>
    <name>framework/DataObjects.test_output_attributes</name>
    <author>alfoa</author>
    <created>2015-06-16</created>
    <classesTested>DataObjects.HistorySet, Models.ExternalModel</classesTested>
    <description>
       Test to check the functionalities of DataObjects preprocess the data applying simple 
       operator (e.g. max value, average, min value, etc.), in the $options$ XML node.
    </description>
    <revisions>
      <revision author="maljdan" date="2016-06-23">Moving test case into a specific DataObjects folder</revision>
      <revision author="cogljj" date="2016-08-23">Converting test dataobjects attributes to external model. Refs #94</revision>
      <revision author="alfoa" date="2017-01-21">Adding this test description.</revision>
      <revision author="maljdan" date="2017-07-31">Splitting original test case file (test_dataobjects_attributes.xml or framework/DataObjects.test_output_attributes) into more granular test cases</revision>
    </revisions>
  </TestInfo>

  <ExternalXML node="RunInfo" xmlToLoad="DataobjectsAttributes/RunInfo.xml"/>
  <ExternalXML node="Models" xmlToLoad="DataobjectsAttributes/Model.xml"/>
  <ExternalXML node="Distributions" xmlToLoad="DataobjectsAttributes/Distributions.xml"/>
  <ExternalXML node="Samplers" xmlToLoad="DataobjectsAttributes/Sampler.xml"/>

  <Steps>
    <MultiRun name="TestDataObjectAttributes">
      <Input class="DataObjects" type="PointSet">inputHolder</Input>
      <Model class="Models" type="ExternalModel">PythonModule</Model>
      <Sampler class="Samplers" type="Grid">myGrid</Sampler>
      <Output class="DataObjects" type="HistorySet">allStories</Output>
      <Output class="DataObjects" type="HistorySet">stories1</Output>
      <Output class="DataObjects" type="HistorySet">stories2</Output>
      <Output class="OutStreams" type="Print">allStories_dump</Output>
      <Output class="OutStreams" type="Print">stories1_dump</Output>
      <Output class="OutStreams" type="Print">stories2_dump</Output>
    </MultiRun>
  </Steps>

  <ExternalXML node="OutStreams" xmlToLoad="DataobjectsAttributes/OutStreams.xml"/>
  <ExternalXML node="DataObjects" xmlToLoad="DataobjectsAttributes/DataObjects.xml"/>

</Simulation>
